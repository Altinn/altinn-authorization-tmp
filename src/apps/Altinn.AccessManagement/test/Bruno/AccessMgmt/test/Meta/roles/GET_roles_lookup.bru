meta {
  name: GET_roles_lookup
  type: http
  seq: 3
}

get {
  url: {{baseUrl}}/accessmanagement/api/v1/meta/info/roles?key=legacycode&value=dagl
  body: none
  auth: inherit
}

params:query {
  key: legacycode
  value: dagl
}

tests {
  test("GET_roles_lookup", function() {
    const data = res.getBody();  
    expect(res.status).to.equal(200);
   const daglcode = data.find(item => item.code === 'daglig-leder');
         expect(daglcode).to.exist;
      const daglurn = data.find(item => item.urn === 'urn:altinn:external-role:ccr:daglig-leder');
         expect(daglurn).to.exist;  
  });
  
}
