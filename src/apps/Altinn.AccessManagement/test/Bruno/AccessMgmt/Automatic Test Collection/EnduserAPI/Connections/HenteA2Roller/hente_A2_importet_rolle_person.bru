meta {
  name: hente_A2_importet_rolle_person
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/accessmanagement/api/v1/enduser/connections/roles?party={{party}}&from={{from}}&to={{to}}
  body: none
  auth: inherit
}

params:query {
  party: {{party}}
  from: {{from}}
  to: {{to}}
}

script:pre-request {
  const sharedtestdata = require(`./Testdata/sharedtestdata.json`);
  const testdata = require(`./Testdata/enduser/${bru.getEnvVar("tokenEnv")}.json`);
  
  bru.setVar("party", testdata.hente_A2_roller_from_org.partyuuid);
  bru.setVar("from", testdata.hente_A2_roller_from_org.partyuuid);
  bru.setVar("to", testdata.to_bruker_fikk_rolle.partyuuid);
  
  var getTokenParameters = {
    auth_userId: testdata.hente_A2_roller_from_org.dagligleder.userid,
    auth_partyId: testdata.hente_A2_roller_from_org.dagligleder.partyid,
    auth_partyUuid: testdata.hente_A2_roller_from_org.dagligleder.partyuuid,
    auth_ssn: testdata.hente_A2_roller_from_org.dagligleder.pid,
    auth_tokenType: sharedtestdata.authTokenType.personal,
    auth_scopes: sharedtestdata.auth_scopes.portalEnduser
  }
  
  const token = await testTokenGenerator.getToken(getTokenParameters);
  bru.setVar("bearerToken",  token);
}

