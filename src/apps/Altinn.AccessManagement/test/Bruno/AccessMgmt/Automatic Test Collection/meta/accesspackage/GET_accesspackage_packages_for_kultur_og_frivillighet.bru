params:path {
  id: 5996ba37-6db0-4391-8918-b1b0bd4b394b
}

tests {
  test("GET_accesspackage_packages_for_kultur_og_frivillighet", function() {
    var data = res.getBody();  
    expect(res.status).to.equal(200);
  
    var package = data.find(item => item.name === 'Kunst og underholdning');
    expect(package, 'Did not find "Kunst og underholdning"').to.exist;
    package = data.find(item => item.name === 'Biblioteker, museer, arkiver og annen kultur');
    expect(package, 'Did not find "Biblioteker, museer, arkiver og annen kultur"').to.exist;
    package = data.find(item => item.name === 'Lotteri og spill');
    expect(package, 'Did not find "Lotteri og spill"').to.exist;
    package = data.find(item => item.name === 'Politikk');
    expect(package, 'Did not find "Politikk"').to.exist;
    package = data.find(item => item.name === 'Fornøyelser');
    expect(package, 'Did not find "Fornøyelser"').to.exist;
    package = data.find(item => item.name === 'Sport og fritid');
    expect(package, 'Did not find "Sport og fritid"').to.exist;
  });
}
