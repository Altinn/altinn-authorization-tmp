{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TelemetryConfig": {
    "ServiceName": "api"
  },
  "DbObjDefConfig": {
    "BaseSchema": "dbo",
    "TranslationSchema": "translation",
    "HistorySchema": "history"
  },
  "DataService": {
    "ConnectionString": "<DataService:ConnectionString>"
  },
  "DbMigration": {
    "ConnectionString": "<DbMigration:ConnectionString>",
    "CollectionId": "AccessPackages",
    "DefaultSchema": "dbo",
    "TranslationSchema": "translation",
    "HistorySchema": "history",
    "Enable": true
  },
  "JsonIngest": {
    "Languages": [
      "nno",
      "eng"
    ],
    "Enabled": {
      "RolePackagesIngestService": true,
      "areasAndPackagesIngestService": true,
      "providerIngestService": true,
      "entityTypeIngestService": true,
      "entityVariantIngestService": true,
      "roleIngestService": true,
      "roleMapIngestService": true,
      "tagGroupIngestService": true,
      "tagIngestService": true,
      "entityVariantRoleIngestService":true,
    }
  }
}
